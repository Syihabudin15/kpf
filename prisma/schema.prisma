  rw_domisili        String?
  kelurahan_domisili String?
  kecamatan_domisili String?
  kota_domisili      String?
  provinsi_domisili  String?
  kode_pos_domisili  String?
  geo_location       String?
  DataPengajuan      DataPengajuan[]
}

model DataPengajuanKeluarga {
  id                          String          @id @default(uuid())
  nama_pasangan               String?
  tempat_lahir_pasangan       String?
  alamat_pasangan             String?
  tanggal_lahir_pasangan      String?
  nik_pasangan                String?
  masa_ktp_pasangan           String?
  pekerjaan_pasangan          String?
  nama_keluarga_tidak_serumah String?
  hubungan                    String?
  no_telepon_keluarga         String?
  alamat_keluarga             String?
  DataPengajuan               DataPengajuan[]
}

model BerkasPengajuan {
  id                     String          @id @default(uuid())
  berkas_slik            String?
  berkas_pengajuan       String?
  berkas_idpb            String?
  berkas_flagging        String?
  video_wawancara        String?
  video_asuransi         String?
  berkas_akad            String?
  tanggal_akad           String?
  bukti_cair             String?
  tanggal_bukti_cair     DateTime?
  pelunasan              String?
  tanggal_pelunasan      DateTime?
  jaminan                String?
  tanggal_jaminan        DateTime?
  rekening               String?
  tanggal_rekening       DateTime?
  mutasi                 String?
  tanggal_mutasi         DateTime?
  flagging               String?
  tanggal_flagging       DateTime?
  video_cair             String?
  tanggal_video_cair     DateTime?
  no_rekening            String?
  nama_bank              String?
  berkas_lainnya         String?
  tanggal_berkas_lainnya DateTime?
  video_cair2            String?
  tanggal_video_cair2    DateTime?
  video_cair3            String?
  tanggal_video_cair3    DateTime?
  epotpen                String?
  tanggal_epotpen        DateTime?
  DataPengajuan          DataPengajuan[]
}

model PenyerahanBerkas {
  id                   String    @id @default(uuid())
  tanggal_cetak        DateTime?
  nomor_surat_berkas   String?
  surat_berkas         String?
  tanggal_surat_berkas DateTime?
  is_active            Boolean   @default(true)

  Bank          Bank?           @relation(fields: [bankId], references: [id])
  bankId        String?
  DataPengajuan DataPengajuan[]
}

model PenyerahanJaminan {
  id              String    @id @default(uuid())
  tanggal_cetak   String?
  nomor_jaminan   String?
  jaminan         String?
  tanggal_jaminan DateTime?
  is_active       Boolean   @default(true)

  Bank          Bank?           @relation(fields: [bankId], references: [id])
  bankId        String?
  DataPengajuan DataPengajuan[]
}

model DataPencairan {
  id                     String    @id @default(uuid())
  tanggal_cetak          DateTime
  nomor_surat            String
  is_active              Boolean   @default(true)
  berkas_si              String?
  tanggal_berkas_si      DateTime?
  bukti_transfer         String?
  tanggal_bukti_transfer DateTime?
  status                 Boolean   @default(false)
  tanggal_proses         DateTime?

  DataPengajuan DataPengajuan[]
  Bank          Bank?           @relation(fields: [bankId], references: [id])
  bankId        String?
}

model User {
  id            String    @id @default(uuid())
  nip           String?   @unique
  nik           String?   @unique
  first_name    String
  last_name     String
  username      String    @unique
  email         String    @unique
  tempat_lahir  String?
  tanggal_lahir DateTime?
  alamat        String?
  no_telepon    String
  password      String
  role          Role
  posisi        String?
  status_pkwt   String?
  status_active Boolean
  picture       String?   @default("/profile/profile_default.svg")
  mulai_kontrak DateTime  @default(now())
  masa_kotrak   Int       @default(3)
  created_at    DateTime  @default(now())
  target        Int       @default(50000000)

  DataPembiayaan DataPembiayaan[]

  unit_cabang_id String?
  UnitCabang     UnitCabang?     @relation(fields: [unit_cabang_id], references: [id])
  DataPengajuan  DataPengajuan[]
  Bank           Bank?           @relation(fields: [bank_id], references: [id])
  bank_id        String?
  Cost           Cost[]
}

model UnitPelayanan {
  id          String   @id @default(uuid())
  name        String   @unique
  kode_area   String   @unique
  number_kode String?
  created_at  DateTime @default(now())

  is_active Boolean @default(true)

  UnitCabang UnitCabang[]
}

model UnitCabang {
  id            String   @id @default(uuid())
  name          String   @unique
  kode_area     String   @unique
  number_code   String?
  alamat_cabang String?
  created_at    DateTime @default(now())

  is_active Boolean @default(true)

  unit_pelayanan_id String?
  UnitPelayanan     UnitPelayanan? @relation(fields: [unit_pelayanan_id], references: [id])
  User              User[]
}

model DataTaspen {
  id                 String        @id @default(uuid())
  nopen              String        @unique
  nama               String
  nama_skep          String?
  no_skep            String?
  kode_jiwa          String
  no_telepon         String?
  nik                String?
  masa_ktp           String?
  npwp               String?
  pendidikan         Pendidikan?
  jenis_kelamin      JenisKelamin?
  agama              Agama?
  masa_kerja         Int?
  status_rumah       StatusRumah?
  menempati_tahun    String?
  nama_ibu_kandung   String?
  pekerjaan_sekarang String?
  alamat_pekerjaan   String?
  jenis_usaha        JenisUsaha?
  status_kawin       StatusKawin?

  nomor_sk_pensiun          String?
  tanggal_sk_pensiun        DateTime?
  tanggal_lahir             String?
  tanggal_wafat             String?
  tanggal_nikah             String?
