generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Role {
  MASTER
  ENTRY_DATA
  BANK
  VERIFIKASI
  CHECKER
  MAKER
  APPROVAL
  MARKETING
  OPERASIONAL
  BISNIS
  PEMBERKASAN
  KEUANGAN
  MANAGEMENT
}

enum StatusPengajuan {
  ANTRI
  DITOLAK
  SETUJU
  PENDING
}

enum StatusPencairan {
  TRANSFER
  PROSES
  BATAL
}

enum Pendidikan {
  SD
  SMP
  SMA
  D3
  S1
  S2
  S3
  LAINNYA
}

enum JenisKelamin {
  LAKI_LAKI
  PEREMPUAN
}

enum Agama {
  ISLAM
  KATHOLIK
  KONGHUCU
  HINDU
  BUDHA
  ATHEIS
  LAINNYA
}

enum StatusRumah {
  SEWA
  MILIK_SENDIRI
  MILIK_KELUARGA
  MILIK_ORANGLAIN
  NGEKOS
  TIDAK_PUNYA_RUMAH
}

enum JenisUsaha {
  WARUNG_KOPI
  TOKO_KELONTONG
  JASA_CUCI_MOBIL_DAN_MOTOR
  KATERING
  LOUNDRY
  SALON_KECANTIKAN
  LAINNYA
}

enum StatusKawin {
  BELUM_KAWIN
  KAWIN
  JANDA
  DUDA
}

enum JenisMargin {
  FLAT
  ANUITAS
}

enum StatusPelunasan {
  TOPUP
  MENINGGAL_DUNIA
  LEPAS
}

model Produk {
  id             String   @id @default(uuid())
  name           String
  by_asuransi    Float
  mg_bunga       Float
  min_age        Float
  max_age        Float
  max_usia_lunas Float
  max_tenor      Int
  max_plafon     Int
  is_active      Boolean  @default(true)
  created_at     DateTime @default(now())

  bank_id        String?
  Bank           Bank?            @relation(fields: [bank_id], references: [id])
  DataPembiayaan DataPembiayaan[]
}

model Bank {
  id               String  @id @default(uuid())
  name             String  @unique
  kode             String? @unique
  by_admin         Float
  by_admin_bank    Float?
  by_lainnya       Float?
  by_tatalaksana   Int
  by_materai       Int
  by_buka_rekening Int
  by_angsuran      Float
  by_flagging      Int     @default(20000)
  by_epotpen       Int     @default(35000)
  by_provisi       Int?    @default(0)
  margin_bank      Float?  @default(14)
  is_syariah       Boolean @default(false)
  is_flash         Boolean @default(false)

  logo              String?
  up_direktur       String?
  direktur          String?
  penanggung_jawab  String?
  account_officer   String?
  credit_review     String?
  ketua_credit      String?
  wakil_ketua       String?
  diperiksa_oleh    String?
  jabatan_diperiksa String?
  otorisasi_oleh    String?
  jabatan_otorisasi String?
  akad              String?
  sk_akad           String?  @db.LongText
  alamat            String?
  no_telepon        String?
  email             String?
  is_active         Boolean  @default(true)
  created_at        DateTime @default(now())
  pembulatan        Int      @default(1)

  products          Produk[]
  User              User[]
  DataPengajuan     DataPengajuan[]
  DataPencairan     DataPencairan[]
  PenyerahanBerkas  PenyerahanBerkas[]
  PenyerahanJaminan PenyerahanJaminan[]
}

model JenisPembiayaan {
  id         String   @id @default(uuid())
  name       String   @unique
  by_mutasi  Int
  is_active  Boolean  @default(true)
  created_at DateTime @default(now())

  DataPembiayaan DataPembiayaan[]
}

model Refferal {
  id         String   @id @default(uuid())
  name       String   @unique
  kode       String?
  alamat     String?
  no_telepon String?
  email      String?
  is_active  Boolean  @default(true)
  created_at DateTime @default(now())

  DataPembiayaan DataPembiayaan[]
}

model DataPembiayaan {
  id                    String   @id @default(uuid())
  nopen                 String
  name                  String
  keterangan            String?
  alamat                String
  tanggal_lahir         String
  tempat_lahir          String?
  juru_bayar_asal       String?
  juru_bayar_tujuan     String?
  pembiayaan_sebelumnya String?
  no_rekening           String?
  nama_bank             String?
  tanggal_input         DateTime @default(now())
  gaji_bersih           Int
  by_tatalaksana        Int
  by_mutasi             Int
  by_provisi            Int
  mg_bunga              Float
  by_admin              Float
  by_admin_bank         Float    @default(0)
  by_lainnya            Float    @default(0)
  by_asuransi           Float
  by_buka_rekening      Int
  by_materai            Int
  by_flagging           Int      @default(20000)
  by_epotpen            Int      @default(35000)
  tenor                 Int
  plafond               Int
  retensi               Int      @default(0)
  blokir                Int      @default(0)
  bpp                   Int      @default(0)
  pelunasan             Int      @default(0)
  fee                   Int      @default(0)
  margin_bank           Int      @default(0)
  pembulatan            Int      @default(100)

  is_simulasi Boolean
  is_active   Boolean @default(true)

  created_at DateTime @default(now())
  updated_at DateTime @default(now())

  Produk              Produk?          @relation(fields: [produk_id], references: [id])
  produk_id           String?
  JenisPembiayaan     JenisPembiayaan? @relation(fields: [jenis_pembiayaan_id], references: [id])
  jenis_pembiayaan_id String?
  User                User?            @relation(fields: [user_id], references: [id])
  user_id             String?
  DataPengajuan       DataPengajuan[]
  Refferal            Refferal?        @relation(fields: [refferal_id], references: [id])
  refferal_id         String?
}

model DataPengajuan {
  id                        String           @id @default(uuid())
  tujuan_penggunaan1        String
  tujuan_penggunaan2        String?
  geo_location              String?
  agent_fronting            String?
  status_slik               StatusPengajuan?
  keterangan_slik           String?          @db.Text
  tanggal_slik              DateTime?
  nama_pemeriksa_slik       String?
  status_verifikasi         StatusPengajuan?
  keterangan_verifikasi     String?          @db.Text
  tanggal_verifikasi        DateTime?
  nama_pemeriksa_verifikasi String?
  status_checker            StatusPengajuan?
  keterangan_checker        String?          @db.Text
  tanggal_checker           DateTime?
  nama_pemeriksa_checker    String?
  status_maker              StatusPengajuan?
  keterangan_maker          String?          @db.Text
  tanggal_maker             DateTime?
  nama_pemeriksa_maker      String?
  status_approval           StatusPengajuan?
  keterangan_approval       String?          @db.Text
  tanggal_approval          DateTime?
  nama_pemeriksa_approval   String?
  status_pencairan          StatusPencairan?
  tanggal_pencairan         DateTime?
  tanggal_cetak_akad        DateTime?
  nomor_akad                String?
  jenis_margin              JenisMargin?
  is_active                 Boolean          @default(true)

  nopen         String?
  nama          String?
  nama_skep     String?
  kode_jiwa     String?
  golongan      String?
  jenis_pensiun String?
  nik           String?

  masa_ktp           String?
  npwp               String?
  pendidikan         Pendidikan?
  jenis_kelamin      JenisKelamin?
  agama              Agama?
  masa_kerja         Int?
  status_rumah       StatusRumah?
  menempati_tahun    String?
  nama_ibu_kandung   String?
  pekerjaan_sekarang String?
  alamat_pekerjaan   String?
  jenis_usaha        JenisUsaha?
  status_kawin       StatusKawin?

  nomor_sk_pensiun   String?
  tanggal_sk_pensiun String?
  tmt_pensiun        String?
  penerbit_sk        String?
  no_telepon         String?

  is_cetak                    Boolean   @default(false)
  pembayaran_asuransi         Boolean   @default(false)
  status_lunas                Boolean   @default(false)
  tanggal_pembayaran_asuransi DateTime?
  area_pelayanan_berkas       String?

  BerkasPengajuan   BerkasPengajuan? @relation(fields: [berkasPengajuanId], references: [id])
  berkasPengajuanId String?

  DataTaspen   DataTaspen? @relation(fields: [dataTaspenId], references: [id])
  dataTaspenId String?

  User    User?   @relation(fields: [user_id], references: [id])
  user_id String?

  data_pembiayaan_id      String
  DataPembiayaan          DataPembiayaan        @relation(fields: [data_pembiayaan_id], references: [id])
  Bank                    Bank?                 @relation(fields: [bankId], references: [id])
  bankId                  String?
  JadwalAngsuran          JadwalAngsuran[]
  DataPencairan           DataPencairan?        @relation(fields: [dataPencairanId], references: [id])
  dataPencairanId         String?
  DataPengajuanPasangan   DataPengajuanKeluarga @relation(fields: [dataPengajuanKeluargaId], references: [id])
  dataPengajuanKeluargaId String

  DataPengajuanAlamat   DataPengajuanAlamat? @relation(fields: [dataPengajuanAlamatId], references: [id])
  dataPengajuanAlamatId String?
  PenyerahanBerkas      PenyerahanBerkas?    @relation(fields: [penyerahanBerkasId], references: [id])
  penyerahanBerkasId    String?
  PenyerahanJaminan     PenyerahanJaminan?   @relation(fields: [penyerahanJaminanId], references: [id])
  penyerahanJaminanId   String?
  PelunasanDebitur      PelunasanDebitur[]
}

