  User   User?   @relation(fields: [userId], references: [id])
  userId String?
}

model MemoInternal {
  id          String @id @default(uuid())
  title       String
  description String @db.Text
}

model PelunasanDebitur {
  id                String          @id @default(uuid())
  type              StatusPelunasan
  by_admin          Int
  sisa_pokok        Int
  no_rekening       String
  nama_bank         String
  keterangan        String
  tanggal_pelunasan DateTime
  berkas_pelunasan  String

  DataPengajuan   DataPengajuan @relation(fields: [dataPengajuanId], references: [id])
  dataPengajuanId String
}

model flagging {
  id                   Int     @id @default(autoincrement())
  status_peserta       String  @db.VarChar(10)
  notas                String  @db.VarChar(20)
  kantor_cabang        String  @db.VarChar(20)
  alamat_cabang        String  @db.VarChar(60)
  nipnrp               String  @db.VarChar(20)
  nama_penerima        String  @db.VarChar(40)
  jandadudaypdari      String  @db.VarChar(40)
  alamatrumah          String  @db.VarChar(80)
  penerbitskep         String? @db.VarChar(10)
  noskep               String  @db.VarChar(40)
  jnsdapem             String  @db.VarChar(10)
  jnspens              String  @db.VarChar(40)
  ktrbaydapem          String  @db.VarChar(60)
  norek                String  @db.VarChar(20)
  stsdapem             String  @db.VarChar(50)
  tgllahir_penerima    String  @db.VarChar(10)
