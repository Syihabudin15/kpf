model GiroBank {
  id          String        @id @default(uuid())
  name        String        @unique
  slug        String        @unique
  description String?       @db.Text
  Transaction Transaction[]
}

enum TransactionType {
  MASUK
  KELUAR
}

model Transaction {
  id          String          @id @default(uuid())
  type        TransactionType
  nominal     Int             @default(0)
  description String?         @db.Text
  created_at  DateTime        @default(now())
  updated_at  DateTime?

  GiroBank          GiroBank         @relation(fields: [giroBankId], references: [id])
  giroBankId        String
  OutcomeCategory   OutcomeCategory? @relation(fields: [outcomeCategoryId], references: [id])
  outcomeCategoryId String?
}

model OutcomeCategory {
  id          String  @id @default(uuid())
  name        String  @unique
  description String?
  is_active   Boolean @default(true)

  Transaction Transaction[]
}
